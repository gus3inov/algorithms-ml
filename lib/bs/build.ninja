bs_package_flags = -bs-package-map algorihtms
src_root_dir = C:\OSPanel\domains\node-app
bsc = C:\Users\user\AppData\Roaming\npm\node_modules\bs-platform\lib\bsc.exe
bsdep = C:\Users\user\AppData\Roaming\npm\node_modules\bs-platform\lib\bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101 -warn-error +101
bsc_flags = -bs-suffix -nostdlib -I "C:\OSPanel\domains\node-app\node_modules\bs-platform\lib\ocaml" -color always
ppx_flags = 
bs_package_includes = 
bs_package_dev_includes = 
namespace = -ns Algorihtms
bsb_dir_group = 0
refmt = C:\Users\user\AppData\Roaming\npm\node_modules\bs-platform\lib\refmt.exe
reason_react_jsx = 
refmt_flags = --print binary
bsc_lib_includes = -I . -I src\algorithms\01_binary_search
rule build_ast_and_module_sets_from_re
  command = ${bsc} -pp "${refmt} ${refmt_flags}" ${reason_react_jsx}  ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast -impl ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src\algorithms\01_binary_search\binary_search.mlast : build_ast_and_module_sets_from_re $src_root_dir\src\algorithms\01_binary_search\binary_search.re
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src\algorithms\01_binary_search\binary_search.mlast.d : build_deps src\algorithms\01_binary_search\binary_search.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src\algorithms\01_binary_search\binary_search-Algorihtms.cmj |  $src_root_dir\src\algorithms\01_binary_search\binary_search.bs.js src\algorithms\01_binary_search\binary_search-Algorihtms.cmi : build_cmj_cmi src\algorithms\01_binary_search\binary_search.mlast
  bsc_flags = $bsc_flags -bs-re-out -bs-super-errors
  bs_package_flags = $bs_package_flags  -bs-package-output commonjs:src\algorithms\01_binary_search
rule build_package
  command = ${bsc} -w -49 -no-alias-deps -c ${in}
  description = [34mBuilding[39m [2m${out}[22m
build Algorihtms.cmi : build_package Algorihtms.mlmap
build build.ninja : phony  ||  Algorihtms.cmi src\algorithms\01_binary_search\binary_search.mlast.d
